
@{
    ViewBag.Title = "Cadastro dos Tipos de Animal";
}
<section class="titlepages">
    <h2>Registro das Raças </h2>
</section>

<form action="confirma" method="post">
    <label>Código da Raça:</label>

    <input type="number" name="txtCod" id="txtCod" />


    <label>Tipo de Animal:</label>

    <input type="text" name="txtTipo" id="txtTipo" />


    <input type="submit" name="btnCadastro" id="btnCadastro" value="CADASTRAR" />
</form>
